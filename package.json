{
   "name":"GithubTest",
   "version":"1.0.0",
   "description":"Test PM2 Deploys",
   "main":"app.js",
   "dependencies":{
      "pmx":"^0.3.4"
   },
   "devDependencies":{

   },
   "scripts":{
      "test":"echo \"Error: no test specified\" && exit 1"
   },
   "repository":{
      "type":"git",
      "url":"https://github.com/Anthbs/GithubTest.git"
   },
   "keywords":[
      "PM2",
      "Github"
   ],
   "author":"Anthbs",
   "license":"ISC",
   "bugs":{
      "url":"https://github.com/Anthbs/GithubTest/issues"
   },
   "homepage":"https://github.com/Anthbs/GithubTest",
   "apps":[
      {
         "name":"GithubTest",
         "script":"app.js",
         "env":{
            "COMMON_VARIABLE":"true"
         },
         "env_production":{
            "NODE_ENV":"production"
         }
      }
   ],
   "deploy":{
      "production":{
         "user":"administrator",
         "host":"127.0.0.1",
         "ref":"origin/master",
         "repo":"https://github.com/Anthbs/GithubTest.git",
         "path":"/home/administrator/NodeApps/Githubtest/",
         "post-deploy":"npm install && pm2 startOrRestart GithubTest  --env production"
      }
   }
}
